apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: {{ kubernetes_pki_directory }}/ca.pem
    server: {{ kube_api_server }}:{{ kube_api_port }}
  name: bootstrap
contexts:
- context:
    cluster: bootstrap
    user: kubelet-bootstrap
  name: bootstrap
current-context: bootstrap
preferences: {}
users:
- name: kubelet-bootstrap
  user:
    token: {{ bootstrap_token }}